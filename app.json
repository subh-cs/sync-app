{
  "expo": {
    "name": "sync-app",
    "slug": "sync-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/synclabs_icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splashscreen_portrait.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/synclabs_icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/synclabs_icon.png"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ]
  }
}
